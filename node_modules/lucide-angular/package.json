{
  "name": "lucide-angular",
  "description": "A Lucide icon library package for Angular applications",
  "version": "0.298.0",
  "author": "SMAH1",
  "license": "ISC",
  "homepage": "https://lucide.dev",
  "bugs": "https://github.com/lucide-icons/lucide/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucide-icons/lucide.git",
    "directory": "packages/lucide-angular"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "keywords": [
    "Lucide",
    "Angular",
    "Feather",
    "Icons",
    "Icon",
    "SVG",
    "Feather Icons",
    "Fontawesome",
    "Font Awesome"
  ],
  "peerDependencies": {
    "@angular/common": "13.x - 17.x",
    "@angular/core": "13.x - 17.x"
  },
  "module": "fesm2015/lucide-angular.mjs",
  "es2020": "fesm2020/lucide-angular.mjs",
  "esm2020": "esm2020/lucide-angular.mjs",
  "fesm2020": "fesm2020/lucide-angular.mjs",
  "fesm2015": "fesm2015/lucide-angular.mjs",
  "typings": "lucide-angular.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./lucide-angular.d.ts",
      "esm2020": "./esm2020/lucide-angular.mjs",
      "es2020": "./fesm2020/lucide-angular.mjs",
      "es2015": "./fesm2015/lucide-angular.mjs",
      "node": "./fesm2015/lucide-angular.mjs",
      "default": "./fesm2020/lucide-angular.mjs"
    },
    "./src/icons": {
      "types": "./src/icons/lucide-angular-src-icons.d.ts",
      "esm2020": "./esm2020/src/icons/lucide-angular-src-icons.mjs",
      "es2020": "./fesm2020/lucide-angular-src-icons.mjs",
      "es2015": "./fesm2015/lucide-angular-src-icons.mjs",
      "node": "./fesm2015/lucide-angular-src-icons.mjs",
      "default": "./fesm2020/lucide-angular-src-icons.mjs"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "^2.3.0"
  }
}