{
  "name": "simplebar-angular",
  "version": "3.2.4",
  "description": "Angular component for SimpleBar",
  "publishConfig": {
    "directory": "dist"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "keywords": [
    "simplebar-angular",
    "angular",
    "simplebar"
  ],
  "dependencies": {
    "simplebar-core": "^1.2.4",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/common": ">=12.0.0",
    "@angular/core": ">=12.0.0"
  },
  "main": "bundles/simplebar-angular.umd.js",
  "module": "fesm2015/simplebar-angular.js",
  "es2015": "fesm2015/simplebar-angular.js",
  "esm2015": "esm2015/simplebar-angular.js",
  "fesm2015": "fesm2015/simplebar-angular.js",
  "typings": "simplebar-angular.d.ts",
  "sideEffects": false
